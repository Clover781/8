        ["Need help?"] = "éœ€è¦å¸®åŠ©?",
        ["CMD-X V2 is in development."] = "CMD-X V2 æ­£åœ¨å¼€å‘ä¸­ã€‚",
        ["Please join the discord for development updates."] = "ä½ çœ‹ä½ å¦ˆå‘¢ğŸ˜ˆ",
        ["You can get the discord invite by using the .discord command."] = "ä½ å¯ä»¥ä½¿ç”¨å‘½ä»¤è·å–Dcé‚€è¯·é“¾æ¥",
